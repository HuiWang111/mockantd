{
  "name": "mockantd",
  "version": "1.0.0",
  "description": "react + typescript仿antd组件",
  "main": "index.js",
  "scripts": {
    "rollup": "NODE_ENV=production rollup -c",
    "start": "webpack-dev-server --config webpack/config.dev.js --inline --watch",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook --docs",
    "compile:cjs": "tsc --declaration --downlevelIteration --skipLibCheck --jsx react --module commonjs --target es5 --outDir dist/cjs",
    "compile:es": "tsc --declaration --downlevelIteration --skipLibCheck --jsx react --module es2015 --target es2015 --outDir dist/es",
    "deploy": "source scripts/deploy.sh",
    "eslint": "eslint src --ext .ts,.tsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/HuiWang111/mockantd.git"
  },
  "keywords": [
    "react",
    "typesectipt",
    "components"
  ],
  "author": "HuiWang111",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HuiWang111/mockantd/issues"
  },
  "homepage": "https://github.com/HuiWang111/mockantd#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.157",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-spellcheck": "0.0.17",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "less": "^3.11.3",
    "less-loader": "^5.0.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "rollup": "^2.35.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "style-loader": "^1.2.1",
    "ts-import-plugin": "^1.6.6",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
